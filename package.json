{
  "name": "my-expenses-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build": "rimraf dist && npx prisma generate && tsc",
    "start": "npx prisma migrate deploy && node dist/index.js",
    "dev": "npx prisma migrate deploy && ts-node src/index.ts",
    "watch": "npx prisma migrate deploy && nodemon --exec ts-node src/index.ts",
    "import-csv": "ts-node src/scripts/importCsv.ts",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "add-build": "git add dist",
    "ts.check": "tsc --project tsconfig.json",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/express": "^5.0.0",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parser": "^3.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "node-fetch": "^3.3.2",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.82.0",
    "prisma": "^5.20.0",
    "reflect-metadata": "^0.2.2",
    "winston": "^3.14.2"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
